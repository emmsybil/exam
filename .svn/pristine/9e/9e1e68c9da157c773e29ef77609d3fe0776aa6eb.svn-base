<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.cw.portal.persistence.DictationMapper">
	<select id="getDictationByDate" resultType="com.cw.common.domain.dictation.Dictation">
		select
		dictationId,studentGroupsId,words,createDate,dictationDate from
		et_dictation
		where dictationDate Between #{startTime} And #{endTime} order By dictationDate desc
	</select>
</mapper>
