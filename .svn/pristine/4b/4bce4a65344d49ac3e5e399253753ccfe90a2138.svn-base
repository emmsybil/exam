package com.cw.common.domain.dictation;

/**
 * 听写表实体
 * 
 * @author 文浩
 *
 */
public class Dictation {
	// 表ID
	private int dictationId;
	// 学生组ID
	private int studentGroupsId;
	// 听写单子
	private String words;
	// 创建时间
	private String createDate;
	// 听写时间
	private String dictationDate;

	public int getDictationId() {
		return dictationId;
	}

	public void setDictationId(int dictationId) {
		this.dictationId = dictationId;
	}

	public int getStudentGroupsId() {
		return studentGroupsId;
	}

	public void setStudentGroupsId(int studentGroupsId) {
		this.studentGroupsId = studentGroupsId;
	}

	public String getWords() {
		return words;
	}

	public void setWords(String words) {
		this.words = words;
	}

	public String getCreateDate() {
		return createDate;
	}

	public void setCreateDate(String createDate) {
		this.createDate = createDate;
	}

	public String getDictationDate() {
		return dictationDate;
	}

	public void setDictationDate(String dictationDate) {
		this.dictationDate = dictationDate;
	}

}
